  su asterisk asterisk
  rotate 4
  weekly
  compress
  missingok
  notifempty
}

/var/log/asterisk/cdr-custom/Master.csv {
  su asterisk asterisk
  rotate 4
  weekly
  compress
  missingok
  notifempty
}
